[{%- if rest_response_getall is defined and rest_response_getall.json is defined  -%}
    {%- for filter_item in rest_response_filters -%}
        {% set filter_loop = loop %}
        {% if rest_response_getall.json.portalGatewayResponse is iterable and (rest_response_getall.json.portalGatewayResponse is not string and rest_response_getall.json.portalGatewayResponse is not mapping) %}
            {%- for responseitem in rest_response_getall.json.portalGatewayResponse -%}
                {%- if responseitem.gatewayName == filter_item.name and responseitem.version == filter_item.version and responseitem.stageName == filter_item.stageName -%}
                    {%- if filter_loop.index0 > 0 -%},{%- endif -%}'{{ responseitem.id }}'
                {%- endif -%}
            {%- endfor -%}
        {%- endif -%}
    {%- endfor -%}
{%- endif -%}]